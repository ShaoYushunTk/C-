// 6.22(StarSquare).cpp : 此文件包含 "main" 函数。程序执行将在此处开始并结束。
//输入边长，输出星号正方形（边长为输入值）

#include<iostream>
using namespace std;

int StarSquare(int);
int main()
{
	int side = 0;
	StarSquare(side);
}
int StarSquare(int size)
{
	cout << "Enter the size of the StarSquare: ";
	cin >> size;
	cout << endl;
	for (int i = 1; i <= size; i++)
	{
		for (int j = 1; j <= size; j++)
		{
			cout << "*";
		}
		cout << endl;
	}
	return 0;
}

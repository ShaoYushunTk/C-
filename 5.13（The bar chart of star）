// 5.13（The bar chart of star）.cpp : 此文件包含 "main" 函数。程序执行将在此处开始并结束。
//输入5个数，打印*，*数=输入的数
#include<iostream>
using namespace std;

int main()
{
	int number;
	for (int counter = 1; counter <= 5; counter++)
	{
		cin >> number;
		for (int j = 1; j <= number; j++)
		{
			cout << "*";
		}
		cout << endl;
	}
}

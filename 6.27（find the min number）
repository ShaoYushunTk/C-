// 6.27（find the min number）.cpp : 此文件包含 "main" 函数。程序执行将在此处开始并结束。
//输入3个数，调用函数输出最小值

#include<iostream>
using namespace std;

double Min(double, double, double);
int main()
{
	double a = 0, b = 0, c = 0;
	cin >> a >> b >> c;
	Min(a, b, c);

}
double Min(double a, double b, double c)
{
	double min = a;
	if (b < min)
		min = b;
	if (c < min)
		min = c;
	cout << min;
	return 0;
}

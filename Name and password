// Name and password.cpp : 此文件包含 "main" 函数。程序执行将在此处开始并结束。
//

#include<iostream>
#include<string>
using namespace std;
 
int main()
{
	string userName;
	string Password;
	int a, b;

	cout << "Please Enter userName: ";
	getline(cin, userName);
	cout << "Please Enter Password: ";
	getline(cin, Password);

	if (userName == "18988918939")
		a = 1;
	else
		a = 0; 

	if (Password == "Sys170835")
		b = 1;
	else
		b = 0;

	if (a == 1 && b == 1)
		cout << "Welcome to SCUT." << endl;
	else if (a == 0)
		cout << "Your userName is wrong." << endl;
	else if (a == 1 && b == 0)
		cout << "Your Password is wrong." << endl;

}
